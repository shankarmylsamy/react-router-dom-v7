{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react",
        "esModuleInterop": true,
        "typeRoots": [
            "./node_modules/@types",
            "./src/types"
        ],
        "plugins": [
            {
                "name": "typescript-plugin-css-modules"
            }
        ]
    },
    "include": [
        "src"
    ]
}